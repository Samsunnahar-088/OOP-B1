import java.util.Scanner;
public class Book
{
	public String title;
	public String author;
	public int price ;

	public Book(String title,String author,int price) {
		this.title=title;
		this.author=author;
		this.price=price;
	}
	public Book(String title,String author) {
		this.title=title;
		this.author=author;
	}
	public Book() {
    System.out.println("Default constructor book is created");
	}


	public void display() {
		System.out.println("Title="+title+" author="+author+" price="+price);
	}

	public static void main(String[] args) {
		Book b1=new Book("habijabi","Sonali",200);
		Book b2=new Book("habijabi 2","Samsunnahar");
		Book b3=new Book();

		b1.display();
		b2.display();
		b3.display();

	}
}
